{
  "name": "vue-component-docs-example",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && sass src/lib/style:dist/style --style compressed && sh scripts/clear-files.sh",
    "build:docs": "vue-tsc --noEmit && vite build --config docs-config.ts",
    "serve": "vite preview"
  },
  "dependencies": {
    "vue": "^3.0.5",
    "vue-router": "4.0.10"
  },
  "devDependencies": {
    "@types/node": "^16.7.6",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "@vue/compiler-sfc": "^3.0.5",
    "sass": "^1.37.5",
    "typescript": "^4.3.2",
    "vite": "^2.4.4",
    "vue-tsc": "^0.2.2"
  }
}
